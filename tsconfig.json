{
    "compilerOptions": {
      "module": "ES2022",
      "target": "ES2022",
      "moduleResolution": "node",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "skipLibCheck": true,
      "sourceMap": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
    }
  }